 <table> 
 	<tr>
      <th>ID</th>
      <th>Link</th>
      <th>Random String</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  <% @urls.each do |url| %>
        <tr>
        	<td><%= url[:id] %></td>
            <td><%= url[:link] %></td>
            <td><%= url[:url] %></td>
          	<td><%=url[:created_at] %></td>
          	<td><%=url[:updated_at] %></td>
        </tr>
    <% end %>
 </table>